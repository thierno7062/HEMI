<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stopcovid19">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.SEND_SMS" />

    <uses-feature
        android:name="android.hardware.telephony"
        android:required="true" />

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StopCovid19">
        <activity android:name=".Controler.NegatifResultatActivity"></activity>
        <activity android:name=".Controler.ResulatPatientActivity" />
        <activity android:name=".Controler.QCMActivity" />
        <activity android:name=".Controler.ResultActivity" />
        <activity android:name=".Controler.TestActivity" />
        <activity android:name=".Controler.ContactActivity" />
        <activity android:name=".Controler.AboutActivity" />
        <activity android:name=".Controler.HomeActivity" />
        <activity android:name=".Controler.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>